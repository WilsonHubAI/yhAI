{
  "name": {
    "en-US": "Get persistent KV",
    "zh-CN": "根据键名持续获取相应的值"
  },
  "desc": {
    "en-US": "Keep getting the corresponding values based on a key.",
    "zh-CN": "根据某个键持续获得对应的值。"
  },
  "type": "actor",
  "adapterClass": "ultrafox/persistentKV",
  "class": "ultrafox/persistentKV#get",
  "inputSchema": {
    "fields": [
      {
        "key": "key",
        "label": {
          "en-US": "Key",
          "zh-CN": "键"
        },
        "desc": {
          "en-US": "Please input the key you want to query.",
          "zh-CN": "请输入需要查找的键。"
        },
        "type": "string",
        "required": true
      },
      {
        "key": "missingAsError",
        "label": {
          "en-US": "Missing as error",
          "zh-CN": "没找到相关信息则报错"
        },
        "desc": {
          "en-US": "Choose whether missing the value is an error. The default choice is True.",
          "zh-CN": "选择若没找到对应值是否报错，默认为True。"
        },
        "type": "bool",
        "required": true,
        "ui": {
          "disableCustomInput": true
        }
      }
    ]
  },
  "outputSchema": {
    "fields": []
  }
}