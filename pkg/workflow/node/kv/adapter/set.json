{
  "name": {
    "en-US": "Set persistent KV",
    "zh-CN": "根据键名持续设置对应的值"
  },
  "desc": {
    "en-US": "Set all values corresponding to the key to the same value.",
    "zh-CN": "将某一键名对应的所有信息设置为同一值。"
  },
  "type": "actor",
  "adapterClass": "ultrafox/persistentKV",
  "class": "ultrafox/persistentKV#set",
  "inputSchema": {
    "fields": [
      {
        "key": "key",
        "label": {
          "en-US": "Key",
          "zh-CN": "键"
        },
        "desc": {
          "en-US": "Please input the key you want to set.",
          "zh-CN": "请输入需要查找的键。"
        },
        "type": "string",
        "required": true
      },
      {
        "key": "value",
        "label": {
          "en-US": "Value",
          "zh-CN": "值"
        },
        "desc": {
          "en-US": "Please input the corresponding value to the key.",
          "zh-CN": "请输入该键对应的值。"
        },
        "type": "string",
        "required": true
      }
    ]
  },
  "outputSchema": {
    "fields": []
  }
}